<Application x:Class="GamdExplorer.Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GamdExplorer.Desktop"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/Colors.xaml" />
                <ResourceDictionary Source="Resource/SvgList.xaml"/>
                <ResourceDictionary Source="Resource/BrushColors.xaml"/>
                
               
                
               

                <ResourceDictionary Source="Resource/Style/Border.xaml"/>
                <ResourceDictionary Source="Resource/Style/TreeView.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
